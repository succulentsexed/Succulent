    <script>
        var app = new Vue({
            // element to mount to
            el: '#app',
            // initial 321 data
            data: {
                youngbegClasses: true,
                begClasses: true,
                begGirlcodeClasses: true,
                advGirlcodeClasses: true,
                advClasses: true,
                appClasses: true,
                allClasses: true,
                hsClasses:true
            },
            methods: {
                isMobile() {
                    if (window.innerWidth <= 700 && window.innerHeight <= 700) {
                        return true;
                    }
                    else {
                        return false;
                    }
                },

                youngbeginnerShow: function() {
                    this.youngbegClasses = true,
                    this.begClasses = false,
                        this.begGirlcodeClasses = false,
                        this.advGirlcodeClasses = false,
                        this.advClasses = false,
                        this.appClasses = false,
                        this.hsClasses=false
                },
                
                beginnerShow: function() {
                    this.youngbegClasses = false,
                    this.begClasses = true,
                        this.begGirlcodeClasses = false,
                        this.advGirlcodeClasses = false,
                        this.advClasses = false,
                        this.appClasses = false,
                        this.hsClasses=false
                },

                begGirlCodeShow: function() {
                     this.youngbegClasses = false,
                    this.begClasses = false,
                        this.begGirlcodeClasses = true,
                        this.advGirlcodeClasses = false,
                        this.advClasses = false,
                        this.appClasses = false,
                        this.hsClasses=false
                },

                advGirlCodeShow: function() {
                     this.youngbegClasses = false,
                    this.begClasses = false,
                        this.begGirlcodeClasses = false,
                        this.advGirlcodeClasses = true,
                        this.advClasses = false,
                        this.appClasses = false,
                      this.hsClasses=false
                },


                advCodeShow: function() {
                     this.youngbegClasses = false,
                     this.begClasses = false,
                        this.begGirlcodeClasses = false,
                        this.advGirlcodeClasses = false,
                        this.advClasses = true,
                        this.appClasses = false,
                      this.hsClasses=false
                },

                appCodeShow: function() {
                     this.youngbegClasses = false,
                    this.begClasses = false,
                        this.begGirlcodeClasses = false,
                        this.advGirlcodeClasses = false,
                        this.advClasses = false,
                        this.appClasses = true,
                      this.hsClasses=false
                },

                showAll: function() {
                     this.youngbegClasses = true,
                     this.begClasses = true,
                        this.begGirlcodeClasses = true,
                        this.advGirlcodeClasses = true,
                        this.advClasses = true,
                        this.appClasses = true,
                      this.hsClasses=false
                },
               hsShow: function() {
                     this.youngbegClasses = false,
                     this.begClasses = false,
                        this.begGirlcodeClasses = false,
                        this.advGirlcodeClasses = false,
                        this.advClasses = false,
                        this.appClasses = false
                    this.hsClasses=true
                },
            }

        })
    </script>
